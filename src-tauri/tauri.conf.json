{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "openvpn-sys",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/SON_hexagon_disconnected.png",
        "icons/SON_hexagon_connected.png",
        "icons/SON_hexagon_intermediate.png",
        "icons/SON_hexagon_error.png",
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.doddi.vpn",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "openvpn-sys",
        "width": 480,
        "height": 200,
        "skipTaskbar": true,
        "visible": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/SON_hexagon_disconnected.png",
      "iconAsTemplate": true
    }
  }
}
